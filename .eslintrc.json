{
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "airbnb",
    "airbnb-typescript"
  ],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/prefer-default-export": "off",
    "react/function-component-definition": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
